    "allowSyntheticDefaultImports": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es2022",
    "jsx": "react",
    "allowJs": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
  }
}
